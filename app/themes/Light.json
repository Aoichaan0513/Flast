{
    "meta": {
        "name": "Light",
        "description": "Simple Light Theme.",
        "version": 1.0,
        "authors": [
            "Flast",
            "Aoichaan0513"
        ]
    },
    "theme": {
        "base": "light",
        "window": {
            "border": {
                "active": "auto",
                "inActive": "#d4d4d4"
            },
            "titleBar": {
                "active": "auto",
                "inActive": "#d4d4d4"
            },
            "foreground": "auto"
        },
        "tab": {
            "background": "auto",
            "foreground": "auto",
            "accent": "auto",
            "hover": {
                "tab": "rgba(196, 196, 196, 0.4)"
            }
        },
        "toolBar": {
            "background": "auto",
            "border": "#0000001f",
            "control": {
                "button": "rgba(130, 130, 130, 0.3)"
            }
        },
        "bookmarkBar": {
            "background": "auto"
        }
    }
}